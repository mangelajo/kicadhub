<%= simple_form_for @kicadnetlist, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :date %>
  <%= f.input :tool %>
  <%= f.file_field :xml %>

  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                kicadnetlists_path, :class => 'btn' %>
  </div>
<% end %>
