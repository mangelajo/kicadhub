<%- model_class = Pcb -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:title) %></th>
      <th><%= model_class.human_attribute_name(:description) %></th>
      <th><%= model_class.human_attribute_name(:file_file_name) %></th>
      <th><%= model_class.human_attribute_name(:file_content_type) %></th>
      <th><%= model_class.human_attribute_name(:file_file_size) %></th>
      <th><%= model_class.human_attribute_name(:file_updated_at) %></th>
      <th><%= model_class.human_attribute_name(:preview_file_name) %></th>
      <th><%= model_class.human_attribute_name(:preview_content_type) %></th>
      <th><%= model_class.human_attribute_name(:preview_file_size) %></th>
      <th><%= model_class.human_attribute_name(:preview_updated_at) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @pcbs.each do |pcb| %>
      <tr>
        <td><%= link_to pcb.id, pcb_path(pcb) %></td>
        <td><%= pcb.title %></td>
        <td><%= pcb.description %></td>
        <td><%= pcb.file_file_name %></td>
        <td><%= pcb.file_content_type %></td>
        <td><%= pcb.file_file_size %></td>
        <td><%= pcb.file_updated_at %></td>
        <td><%= pcb.preview_file_name %></td>
        <td><%= pcb.preview_content_type %></td>
        <td><%= pcb.preview_file_size %></td>
        <td><%= pcb.preview_updated_at %></td>
        <td><%=l pcb.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_pcb_path(pcb), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      pcb_path(pcb),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_pcb_path,
            :class => 'btn btn-primary' %>
