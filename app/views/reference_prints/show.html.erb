<%- model_class = ReferencePrint -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:name) %>:</strong></dt>
  <dd><%= @reference_print.name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:type) %>:</strong></dt>
  <dd><%= @reference_print.type %></dd>
  <dt><strong><%= model_class.human_attribute_name(:assembly_guide_id) %>:</strong></dt>
  <dd><%= @reference_print.assembly_guide_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:reference_id) %>:</strong></dt>
  <dd><%= @reference_print.reference_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:index) %>:</strong></dt>
  <dd><%= @reference_print.index %></dd>
  <dt><strong><%= model_class.human_attribute_name(:file_file_name) %>:</strong></dt>
  <dd><%= @reference_print.file_file_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:file_content_type) %>:</strong></dt>
  <dd><%= @reference_print.file_content_type %></dd>
  <dt><strong><%= model_class.human_attribute_name(:file_file_size) %>:</strong></dt>
  <dd><%= @reference_print.file_file_size %></dd>
  <dt><strong><%= model_class.human_attribute_name(:file_updated_at) %>:</strong></dt>
  <dd><%= @reference_print.file_updated_at %></dd>
  <dt><strong><%= model_class.human_attribute_name(:preview_file_name) %>:</strong></dt>
  <dd><%= @reference_print.preview_file_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:preview_content_type) %>:</strong></dt>
  <dd><%= @reference_print.preview_content_type %></dd>
  <dt><strong><%= model_class.human_attribute_name(:preview_file_size) %>:</strong></dt>
  <dd><%= @reference_print.preview_file_size %></dd>
  <dt><strong><%= model_class.human_attribute_name(:preview_updated_at) %>:</strong></dt>
  <dd><%= @reference_print.preview_updated_at %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              reference_prints_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_reference_print_path(@reference_print), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              reference_print_path(@reference_print),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
